                $("#GenP3").show('fast');
                $("#GenP5").show('fast');
                $("#GenP7").show('fast');
                EmptyDaShit();
                GeneratePartOne();
                RandomSkill();
                GeneratePartThree();
                var WLikeAnswer3 = document.getElementById("WL3").value;
            });
        });
