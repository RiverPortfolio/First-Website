            var GeneratePartThree = function() {
                var WLikeAnswer1 = document.getElementById("WL1").value;
                var WLikeAnswer2 = document.getElementById("WL2").value;
                var WLikeAnswer3 = document.getElementById("WL3").value;
                //alert(WLikeAnswer1);
                
                var RandomNumba = Math.floor(Math.random()*3);
                //alert(RandomNumba);
                if (RandomNumba === 0){
                    $("#GenP6").append(WLikeAnswer1);
                }
                else if (RandomNumba === 1) {
                    $("#GenP6").append(WLikeAnswer2);
                }
                else {
                    $("#GenP6").append(WLikeAnswer3);
                }
            }
            
            //This is for the submit button to do awesome things!
            $("#submit").click(function() {
                //czechParameters();
